int Swap2(int a[2], int l, int h)
{
    int tmp = a[0];
    a[0] = a[1];
    a[1] = tmp;
    return 0;
}


int Swap1(int a[2], int l, int h)
{
    Swap2(a, l, h);
    return 0;
}


int main()
{
    int arr[2];
    arr[0] = 1;
    arr[1] = 2;
    Swap1(arr, 0, 1);
    write(arr[0]);
    write(arr[1]);
    return 0;
}
